load ../common/_aux
load ../../../../deps/dns_formalization/Maude/src/probabilistic-model/_aux

mod IAUX is
  inc AUX + IODINE_NODE .  

  var CONF CONF1 : Config .
  var msg : Msg .

  *** Timeout for Iodine client sending a query(fragment) and not hearing back.
  op ackTimeout : Fragment -> Content .
  *** Timeout for Alice to pace sending a packet to Bob.
  op pktTimeout : Packet -> Content .

  op $delayMsgs : Config Config -> Config .

  --- add delays to messages
  eq delayMsgs(CONF) = $delayMsgs(CONF, null) .
  eq $delayMsgs(null, CONF) = CONF .
  eq $delayMsgs(msg CONF, CONF1) = $delayMsgs(CONF, [delay, msg, drop?] CONF1) .


endm
